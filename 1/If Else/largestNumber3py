a=int(input("Enter The First Number : "))
b=int(input("Enter The Second Number : "))
c=int(input("Enter The Third Number : "))
if(a>b):
    if(a>c):
        print("Larger : ",a)
    else:
        print("Larger : ",c)
elif(b>a):
    if(b>c):
        print("Larger : ",b)
    else:
        print("Larger : ",c)


# Easy Way

largest = max(a, b, c)

print("Largest:", largest)

